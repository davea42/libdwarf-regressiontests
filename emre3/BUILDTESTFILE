g++ -c -gsplit-dwarf main.cc foo.cc
if [  $? -ne 0 ]
then
   echo test g++ exec failed.
   exit 1
fi
rm a.out.dwp
/home/davea/bin/dwp  -o a.out.dwp main.dwo foo.dwo
if [  $? -ne 0 ]
then
   echo test dwp failed.
   exit 1
fi
exit 0

